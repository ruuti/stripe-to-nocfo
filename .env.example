# Stripe Configuration
# Get your Stripe secret key from: https://dashboard.stripe.com/apikeys
STRIPE_SECRET_KEY=sk_live_your_stripe_secret_key_here

# NOCFO Configuration
# Your NOCFO business ID
NOCFO_BUSINESS_ID=your_business_id_here

# Authentication token for NOCFO API
# Get this from your NOCFO account settings
NOCFO_AUTH_TOKEN=your_auth_token_here

# CSRF token for NOCFO API
# Get this from your NOCFO account settings
NOCFO_CSRF_TOKEN=your_csrf_token_here
